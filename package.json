{
  "name": "photoslide",
  "productName": "Photoslide",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/warrengalyen/photoslide.git",
  "author": "Warren Galyen",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve --host 0.0.0.0 --port 3000",
    "build": "rm -rf docs && vue-cli-service build --mode production --no-module",
    "electron-dev": "electron ./main.js",
    "electron-build": "rm -rf dist && electron-builder",
    "start": "http-server docs -a 0.0.0.0 -p 4000 -s"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "build": {
    "appId": "mechanika.photoslide",
    "mac": {
      "icon": "resource/icons/icon.icns",
      "category": "public.app-category.utilities"
    }
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "electron": "^24.3.1",
    "electron-builder": "^22.10.5",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2"
  }
}
